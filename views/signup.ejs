<%-include('partials/header')%>
<div class='container mt-5'>
	<h1>
		Signup
	</h1>
	<form action='/signup' method='post'>
		<div class='form-group'>
			<label for='username' class='m-2'>Username</label>
			<input type='text' name='username' id='username' class='form-control' autofocus required placeholder='example : pavan_kumar'>	
		</div>
		<div class='form-group'>
			<label for='password' class='m-2'>Password</label>
			<input type='password' name='password' id='password' class='form-control' required>
		</div>
		<button type='submit' class='btn btn-primary mt-3'>Signup</button>
	</form> 
</div>
<%-include('partials/footer')%>