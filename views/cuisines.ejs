
	<div class='container'>
		<div class='row mt-5'>
			<%cuisines.forEach((cuisine)=>{%>
			<div class="card" style="width: 20%;" >
				<a href='/dishes'><img class="card-img-top" src=<%=cuisine.img_link%> alt="Card image cap"></a>
  				<div class="card-body">
    				<p class="card-title"><%=cuisine.cuisine%></p>
  				</div>
			</div>
			<%})%>
		</div>
	</div>